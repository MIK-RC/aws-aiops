# ============================================
# AIOps Multi-Agent System - Environment Configuration
# ============================================
# Copy this file to .env and fill in your values:
#   cp .env.example .env
#
# NEVER commit your .env file to version control!
# ============================================

# --------------------------------------------
# AWS Configuration
# --------------------------------------------
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key
AWS_DEFAULT_REGION=us-east-1

# --------------------------------------------
# Bedrock Model Configuration (Optional)
# --------------------------------------------
# Override the default model from config/agents.yaml
# BEDROCK_MODEL=global.anthropic.claude-haiku-4-5-20251001-v1:0

# --------------------------------------------
# DataDog Configuration
# --------------------------------------------
DATADOG_API_KEY=your-datadog-api-key
DATADOG_APP_KEY=your-datadog-app-key
DATADOG_SITE=us5

# --------------------------------------------
# ServiceNow Configuration
# --------------------------------------------
SERVICENOW_INSTANCE=your-instance.service-now.com
SERVICENOW_USER=your-username
SERVICENOW_PASS=your-password

# --------------------------------------------
# Session Storage (Optional)
# --------------------------------------------
# AIOPS_SESSION_BACKEND=s3
# AIOPS_SESSION_BUCKET=your-session-bucket

# --------------------------------------------
# Feature Flags (Optional)
# --------------------------------------------
# AIOPS_DRY_RUN=false
